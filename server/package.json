{
  "name": "ever-life-vault-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && npm run copy-files",
    "build:dev": "tsc --watch",
    "dev": "tsx index.ts",
    "copy-files": "copyfiles -u 0 config/**/* dist/ && copyfiles -u 0 utils/**/* dist/",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.19",
    "@ai-sdk/google": "^2.0.8",
    "@fastify/caching": "^9.0.3",
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/etag": "^6.0.3",
    "@fastify/helmet": "^13.0.1",
    "@fastify/http-proxy": "^10.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/websocket": "^11.2.0",
    "@supabase/supabase-js": "^2.45.4",
    "ai": "^5.0.22",
    "dotenv": "^16.4.5",
    "fastify": "^5.5.0",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "marked": "^12.0.2",
    "node-cron": "^3.0.3",
    "simple-git": "^3.28.0",
    "ssh2": "^1.17.0",
    "yaml": "^2.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.5",
    "@types/node-cron": "^3.0.11",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "vercel": {
    "functions": {
      "index.ts": {
        "maxDuration": 30
      }
    }
  }
}
